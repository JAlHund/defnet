[project]
title = DefNet
version = 0.1
dependencies = https://github.com/britzl/defold-luasocket/archive/0.11.zip,https://github.com/britzl/gooey/archive/6.3.0.zip
custom_resources = examples/assets/www/

[bootstrap]
main_collection = /examples/examples.collectionc

[input]
game_binding = /input/game.input_bindingc

[display]
width = 640
height = 1136

[physics]
scale = 0.02

[script]
shared_state = 1

[library]
include_dirs = defnet

[android]
package = com.defold.defnet

[osx]
bundle_identifier = com.defold.defnet

[ios]
bundle_identifier = com.defold.defnet

